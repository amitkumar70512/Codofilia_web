          <div class="pre">
 .model small
 .data
 opr1 dw 1234h
 opr2 dw 0002h
 result dw 01 dup(?),\'$\'
 .code
         mov ax,@data
         mov ds,ax
         mov ax,opr1
         mov bx,opr2
         clc
         add ax,bx
         mov di,offset result
         mov [di], ax
         
         mov ah,09h
         mov  dx,offset result
         int 21h
         
         mov ah,4ch
         int 21h
         end
